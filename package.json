    "@types/jsonwebtoken": "^9.0.8",
    "@types/mongodb": "^4.0.6",
    "@types/node": "^16.18.50",
    "@types/supertest": "^6.0.2",
    "@types/validator": "^13.12.2",
    "eslint-config-love": "44.0.0",
    "git-commit-msg-linter": "2.3.11",
    "husky": "^9.1.7",
